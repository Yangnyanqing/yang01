# 质数计算器
def zhishu(x):
    #结果变量
    outcome=0
    #小于等于一，表示不是质数。
    if  x <= 1:
        outcome += 1
    else:
        for a in range(1,x+1):
            if x % a == 0:
                if a != 1 and a != x:
                    #outcome>1表示不是质数
                    outcome += 1
        if outcome == 0:
            print(f"{x}是一个质数!")       
        else:
            print(f"{x}不是一个质数!")   
for a in range(0,100):
    zhishu(a)
